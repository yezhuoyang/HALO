q = alloc_data(3)
s = alloc_helper(5)
set_shot(1000)

H q3
CNOT q3, q4
CNOT q4, q5
CNOT q3, q0
CNOT q4, q1
CNOT q5, q2
c0 = MEASURE q3
deallocate_helper(s0)
c1 = MEASURE s1
deallocate_helper(s1)
c2 = MEASURE s2
deallocate_helper(s2)

H s3
CNOT q0, s3
CNOT q2, s3
H s3
c3 = MEASURE s3
deallocate_helper(s3)

H s4
CNOT q1, s4
H s4
c4 = MEASURE s4
deallocate_helper(s4)

deallocate_data(q)